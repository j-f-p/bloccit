<h1>All Questions</h1>

<% @questions.each do |q| %>
    <div class="media">
        <div class="media-body">
            <h4 class="media-heading">
                <%= link_to (q.resolved ? 
                    "&#10003; ".html_safe : "&#9744; ".html_safe) + q.title, 
                    q %>
            </h4>
        </div>
    </div>
<% end %>